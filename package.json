{
  "name": "NowaGUI",
  "version": "1.6.0",
  "description": "A tool for nowa developement.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron ./app/main/index.js",
    "postinstall": "cd app && npm i --registry=http://registry.npm.alibaba-inc.com --production && cd ..",
    "start:prod": "cross-env NODE_ENV=production electron ./app/main/index.js",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack/dev.config.js ",
    "build": "cross-env BABEL_ENV=production ./node_modules/.bin/webpack --config ./webpack/prod.config.js ",
    "prepack": "node prePacker.js",
    "pack": "build --dir",
    "packer:mac": "npm run prepack && build -m",
    "packer:linux": "npm run prepack && build -l",
    "packer:win": "npm run prepack && build -w",
    "packer:win1": "cross-env ELECTRON_MIRROR=https://npm.taobao.org/mirrors/electron/ node packerWin.js",
    "rebuild": "./node_modules/.bin/electron-rebuild -d=https://gh-contractor-zcbenz.cnpmjs.org/atom-shell/dist/ -m ./app",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "productName": "NowaGUI",
    "appId": "com.alibaba-inc.nowa-gui",
    "asar": false,
    "rebuild": true,
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "static/icon"
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "icon": "static/icon"
    },
    "linux": {
      "target": [
        "deb"
      ]
    }
  },
  "homepage": "https://github.com/nowa-webpack/nowa-gui",
  "author": {
    "name": "Poling",
    "email": "466681097@qq.com"
  },
  "license": "MIT",
  "devDependencies": {
    "antd": "^2.8.3",
    "autoprefixer": "^6.6.1",
    "babel-loader": "^6.4.1",
    "babel-preset-babili": "^0.0.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babili-webpack-plugin": "^0.0.7",
    "chokidar": "^1.6.1",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "dva": "^1.2.1",
    "ejs-loader": "^0.3.0",
    "electron": "^1.6.2",
    "electron-builder": "^12.3.1",
    "electron-pubsub": "^0.2.2",
    "electron-rebuild": "^1.5.7",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "git-config": "^0.0.7",
    "html-webpack-plugin": "^2.28.0",
    "i18n-helper": "^1.2.0",
    "ip": "^1.1.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "moment": "^2.18.1",
    "postcss": "^5.2.10",
    "postcss-import": "^9.1.0",
    "postcss-less": "^0.15.0",
    "postcss-loader": "^1.2.2",
    "postcss-nested": "^1.0.0",
    "precss": "^1.4.0",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-dropzone": "^3.11.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-merge": "^2.6.1"
  },
  "dependencies": {
    "cross-env": "^3.1.4"
  }
}
